    "import ergo\n",
    "import numpyro\n",
    "import jax.numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def model():\n",
    "    x = ergo.flip(0.5, name=\"x\")\n",
    "    y = ergo.flip(0.5, name=\"y\")\n",
    "    ergo.condition(x or y)\n",
    "\n",
    "trace = ergo.run(model, num_samples=100)\n",
    "\n",
    "trace.groupby([\"x\", \"y\"]).size().reset_index(name=\"count\")"
   ]
  }
